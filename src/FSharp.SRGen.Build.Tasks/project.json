{
    "title": "F# String Response Generator - MSBuild Task",
    "version": "3.1.0",
    "authors": ["Don Syme","Enrico Sada","Jared Hester"],
    "buildOptions": {        
        "outputName": "FSharp.SRGen.Build.Tasks",
        "additionalArguments": [ "--standalone" ],
        "compilerName": "fsc",
        "compile": {
            "includeFiles": [
                "FsSrGen.fs"
            ]
        }
    },
    "packOptions": {
        "tags":[ "F#","FSharp","netcore","tooling","build" ],
        "files": {
            "mappings": {
                "content/": "FSharp.SRGen.targets"
            }
        },
        "repository": {
            "type": "git",
            "url": "https://github.com/fsprojects/FsSrGen"
        },
        "licenseUrl": "https://github.com/fsprojects/FsSrGen/blob/master/LICENSE.md" 
    },
    "tools": {
        "dotnet-compile-fsc": "1.0.0-preview2-*"
    },
    "configurations": {
        "Release": {
            "buildOptions": {
                "optimize": true
            }
        },
        "Debug":{}        
    },    
    "frameworks": {
        "net46" : {
            "dependencies": {
                "FSharp.Core": "4.0.0.1"
            }, 
            "frameworkAssemblies": {
                "Microsoft.Build.Framework": "",
                "Microsoft.Build.Utilities.v4.0": ""
            }
        }
        ,"netstandard1.6":{
            "dependencies": {
                "NETStandard.Library": "1.6.0",
                "microsoft.fsharp.core.netcore": "1.0.0-*",
                "Microsoft.Build": "14.3.0",
                "Microsoft.Build.Utilities.Core": "14.3.0"
            }
        }
    }
}
