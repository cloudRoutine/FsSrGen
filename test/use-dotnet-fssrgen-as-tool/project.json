{
    "version": "1.0.0",
    "buildOptions": {
        "compilerName": "fsc",
        "compile": {
            "includeFiles": [
                "FSComp.fs",
                "Program.fs"
            ]
        },
        "embed": {
            "includeFiles": [
                "FSComp.resx"
            ]
        }
    },
    "tools": {
        "dotnet-fssrgen": { "version": "3.3.*" },
        "dotnet-compile-fsc": {
            "version": "1.0.0-preview2-*",
            "imports": "dnxcore50"
        }
    },
    "testRunner": "xunit",
    "dependencies": {
        "NETStandard.Library": "1.6.0",
        "xunit": "2.2.0-*",
        "dotnet-test-xunit": "2.2.0-*",
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
        "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.1"
        }
    },
    "frameworks": {
        "netcoreapp1.0": {}
    }
