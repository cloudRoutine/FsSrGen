{
    "title": "F# String Response Generator",
    "version": "3.1.0",
    "authors": ["Don Syme","Enrico Sada","Jared Hester"],
    "buildOptions": {
        "outputName": "fssrgen",
        "emitEntryPoint": true,
        "compilerName": "fsc",
        "additionalArguments": [ "--standalone" ],
        "compile": {
            "includeFiles": [
                "Program.fs"
            ]            
        }
    },
    "packOptions": {
        "tags":[ "F#","FSharp","netcore","tooling","build" ],
        "repository": {
            "type": "git",
            "url": "https://github.com/fsprojects/FsSrGen"
        },
        "licenseUrl": "https://github.com/fsprojects/FsSrGen/blob/master/LICENSE.md"
    },    
    "tools": {
        "dotnet-compile-fsc": {
            "version":"1.0.0-preview2-*",
            "imports": "dnxcore50"
        }
    },   
    "configurations": {
        "Release": {
            "buildOptions": {
                "optimize": true
            }
        },
        "Debug":{}        
    },    
    "frameworks": {
        "net46" : {
            "dependencies": {
                "FSharp.Core": "4.0.0.1"
            },
            "frameworkAssemblies": {
                "System.Xml": ""
            }        
        },
        "netcoreapp1.0": {
            "imports": "dnxcore50",
            "dependencies": {
                "System.Xml.XmlDocument": "4.0.1",
                "System.Reflection":"4.1.0",             	
                "System.Reflection.Extensions":"4.0.1",
                "Microsoft.Fsharp.Core.netcore": "1.0.0-alpha-*",
                "Microsoft.NETcore.App":{
                    "type":"platform",
                    "version":"1.0.1"
                },
                "NETStandard.Library": "1.6.0"
            }            
        }       
    }
}
